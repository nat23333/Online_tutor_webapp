{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "$1"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install",
    "env": {
        "NEXT_PUBLIC_API_URL": "https://online-tutor-webapp.onrender.com",
        "NEXTAUTH_URL": "https://onlinetutorwebapp.vercel.app"
    }
}